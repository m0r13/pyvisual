                "i_enabled": 1.0,
                "i_force_change": 42.0,
                "i_fragment_source": "\n#include <generate/base_time_mask.frag>\n// Shader downloaded and adapted from glslsandbox.com\n// URL: http://glslsandbox.com/e#49147.0\n\n#ifdef GL_ES\nprecision mediump float;\n#endif\n\n//#extension GL_OES_standard_derivatives : disable\n\n\nuniform vec2 mouse;\n;\n\n\n// Ziad 12 / 09 / 2018\n// Leon 05 / 07 / 2017\n// using lines of code of IQ, Mercury, LJ, Koltes, Duke\n\n#define PI 3.14159\n#define TAU PI*2.\n\n#define DITHER\n#define STEPS 30.\n#define BIAS 0.00001\n#define DIST_MIN 0.01\n\nmat2 rot (float a) { float c=cos(a),s=sin(a);return mat2(c,-s,s,c); }\nfloat sphere (vec3 p, float r) { return length(p)-r; }\nfloat cyl (vec2 p, float r) { return length(p)-r; }\nfloat torus( vec3 p, vec2 s ) {\n  vec2 q = vec2(length(p.xz)-s.x,p.y);\n  return length(q)-s.y;\n}\nfloat smin (float a, float b, float r) {\n    float h = clamp(.5+.5*(b-a)/r,0.,1.);\n    return mix(b,a,h)-r*h*(1.-h);\n}\nfloat rand(vec2 co) { return fract(sin(dot(co*0.123,vec2(12.9898,78.233))) * 43758.5453); }\nvec3 camera (vec3 p) {\n    p.yz *= rot((PI*(mouse.y/pyvisualResolution.y-.5)*step(0.5,mouse.x)));\n    p.xz *= rot((PI*(mouse.x/pyvisualResolution.x-.5)*step(0.5,mouse.x)));\n    p.yz *= rot(-PI*.5);\n    return p;\n}\n\nfloat opDisplace( vec3 p )\n{\n    float d1 = torus(p,vec2(.4,.3));\n    float d2 = cos(2.*p.x)*sin(2.*p.y)*sin(2.*p.z);\n    return d1+d2;\n}\n\t\nfloat opTwist( vec3 p )\n{\n    float c = cos(0.0*p.y);\n    float s = sin(1.0*p.y);\n    mat2  m = mat2(c,-s,s,c);\n    vec3  q = vec3(m*p.xz,p.y);\n    return opDisplace(p);\n}\n\nfloat map (vec3 p) {\n    vec3 p1 = p;\n    float geo = 1.;\n    float cy = 0.4;\n    const float repeat = 2.;\n    p1.xy *= rot(length(p)*.5);\n\t\t\n\t\n    float t = pyvisualTime*0.3;\n\t\n    for (float i = 0.; i < repeat; ++i) {\n        p1.yz *= rot(0.3+t*0.5);\n        p1.xy *= rot(0.2+t);\n        p1.xz *= rot(.15+t*2.);\n        \n       \tp1.xy *= rot(p.x*.5+t);\n        \n        // gyroscope\n      //  geo = smin(geo, (torus(p1,vec2(1.+i*.2,.12))), .3);\n        \n\t \n        // tentacles cylinders\n        geo = smin(geo, (opTwist(p1)), .6);\n        \n        // torus along the cylinders\n        vec3 p2 = p1;\n        p2.y = mod(p2.y,cy)-cy/2.;\n        geo = smin(geo, (opTwist(p2)), .1);\n    }\n    return geo;\n}\n\n\n\n\n\nvoid generateFrag() {\n\n\n\n\t\n\t\n\tvec2 uv = ((vec2(pyvisualUV.x, 1.0 - pyvisualUV.y) * pyvisualResolution).xy-.5*pyvisualResolution.xy)/pyvisualResolution.y;\n    vec3 eye = camera(vec3(uv,-2));\n    vec3 ray = camera(normalize(vec3(uv,.75)));\n    vec3 pos = eye;\n    float shade = 0.;\n    #ifdef DITHER\n\tvec2 dpos = ( (vec2(pyvisualUV.x, 1.0 - pyvisualUV.y) * pyvisualResolution).xy / pyvisualResolution.xy );\n\tvec2 seed = dpos + fract(pyvisualTime);\n\t#endif \n    for (float i = 0.; i < STEPS; ++i) {\n\t\tfloat dist = map(pos);\n        if (dist < BIAS) {\n            shade += 0.9;\n        }\n        #ifdef DITHER\n        dist=abs(dist)*(0.1+0.2*rand(seed*vec2(i)));\n        #endif \n        dist = max(DIST_MIN,dist);\n        pos += ray*dist;\n    }\n\tpyvisualOutColor =vec4(1.);\n\tpyvisualOutColor.rgb = vec3(shade/(STEPS-1.),shade*0.9/(STEPS-1.),shade*0.9/(STEPS-1.));\n\t\n}\n",
